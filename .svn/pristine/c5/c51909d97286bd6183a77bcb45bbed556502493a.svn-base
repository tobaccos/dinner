/* 公共样式 */
* {
  margin: 0;
  padding: 0;
  text-decoration: none;
  list-style: none;
}
.lt{
	float: left;
}
.rt{
	float: right;
}
.clearfix{
	clear: both;
}
body {
  	font: 0.29333333rem '微软雅黑';
  	color: #666666;
  	background: url(../img/common/bg.png);
}
.warper{
	width: 680/72rem;
	margin: 0 auto;
}
/* 头部 */
header{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100/72rem;
	background: #017e5c;
}
header h1{
	text-align: center;
	display: inline-block;
	height: 100/72rem;
	line-height: 100/72rem;
	font-size: 35/72rem;
	color: #ffffff;
	width: (720-120-90)/72rem;
}
header .l{
	width: 120/72rem;
	height: 100/72rem;
}
header .r{
	width: 90/72rem;
	height: 100/72rem;
}
/*
header .l a{
	display: inline-block;
	width: 100%;
	height: 100/72rem;
	background: #f2bd47 url("../img/common/xiangxiaJT.png") no-repeat;
	background-size: 120/72rem 100/72rem;
}
*/
header .r a{
	display: inline-block;
	width: 100%;
	height: 100%;
	background: url("../img/common/sousuo.png") no-repeat;
	background-size: 90/72rem 100/72rem;
}
/* 内容 */
.content{
	width: 100%;
	position: absolute;
	top: 100/72rem;
	bottom: 120/72rem;
	overflow: auto;
}
/* 尾部 */
footer{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 120/72rem;
	background: #eeeeee;
}
footer ul li{
	float: left;
	width: 25%;
}
footer ul li a{
	text-align: center;
	width: 100%;
	height: 120/72rem;
	display: inline-block;
	line-height: 170/72rem;
	font-size: 25/72rem;
	color: #a6a6a6;
	background: url("../img/common/shouye.png") no-repeat;
	background-size: 180/72rem 70/72rem;
}
footer ul>li:nth-child(2) a{
	background: url("../img/common/fujin.png") no-repeat;
	background-size: 180/72rem 70/72rem;
}
footer ul>li:nth-child(3) a{
	background: url("../img/common/yonghu.png") no-repeat;
	background-size: 180/72rem 70/72rem;
}
footer ul>li:nth-child(4) a{
	background: url("../img/common/gengduo.png") no-repeat;
	background-size: 180/72rem 70/72rem;
}
