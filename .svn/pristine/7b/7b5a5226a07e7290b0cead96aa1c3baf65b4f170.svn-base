header .r a {
  display: inline-block;
  width: 100%;
  height: 100%;
  background: url("../img/message/setting.png") center no-repeat;
  background-size: 0.76388889rem 0.625rem;
}
.cont {
  width: 100%;
  position: absolute;
  top: 1.38888889rem;
  bottom: 1.66666667rem;
  overflow: auto;
}
.cont .cont_head {
  width: 100%;
  height: 6.45833333rem;
  background: url("../img/banner.png") no-repeat;
  background-size: 10rem 6.45833333rem;
}
.cont_head_img {
  width: 2.91666667rem;
  height: 2.5rem;
  margin: 0 auto;
  text-align: center;
}
.cont_head_img img {
  padding-top: 0.83333333rem;
}
.cont_head_txt {
  width: 100%;
  margin-top: 0.83333333rem;
}
.cont_head_txt p {
  margin: 0 auto;
  text-align: center;
  width: 4.58333333rem;
  height: 0.93055556rem;
  padding-top: 0.20833333rem;
  margin-top: 0.41666667rem;
  line-height: 0.69444444rem;
  background: #cd1c64;
  font-size: 0.31944444rem;
  color: #ffffff;
}
.cont-wraper {
  width: 8.68055556rem;
  margin: 0 auto;
  background: red;
}
.cont_end {
  margin-top: 0.06944444rem;
  margin-bottom: 1.11111111rem;
}
.cont_end ul li {
  margin-top: 0.69444444rem;
  width: 1.875rem;
  height: 2.43055556rem;
  float: left;
  font-size: 0.34722222rem;
  color: #666666;
  text-align: center;
}
.cont_end ul li a p {
  margin-top: 0.34722222rem;
}
.cont_end ul li:nth-child(2) {
  margin-left: 1.52777778rem;
  margin-right: 1.52777778rem;
}
.cont_end ul li:nth-child(5) {
  margin-left: 1.52777778rem;
  margin-right: 1.52777778rem;
}
