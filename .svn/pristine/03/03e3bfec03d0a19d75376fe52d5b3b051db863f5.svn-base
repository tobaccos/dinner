/* 头部 */
header .l a {
  display: inline-block;
  width: 100%;
  height: 100%;
  background: url("../img/waimai/back.png") no-repeat;
  background-size: 1.66666667rem 1.38888889rem;
}
/* 内容 */
.content {
  bottom: 0;
}
.content .content_top {
  width: 100%;
  height: 0.83333333rem;
  background: #eeeeee;
}
.content .content_top ul li {
  float: left;
  width: 2.80555556rem;
  margin-right: 0.51388889rem;
}
.content .content_top ul > li:nth-child(3) {
  margin-right: 0;
}
.content .content_top ul li a {
  display: inline-block;
  background: url("../img/nearby/xuanze.png") no-repeat;
  background-size: 2.80555556rem 0.83333333rem;
  width: 100%;
  line-height: 0.83333333rem;
  font-size: 0.41666667rem;
  color: #000000;
}
.content .content_top ul > li:nth-child(1) a {
  text-indent: 0.625rem;
}
.content .content_top ul > li:nth-child(2) a {
  text-indent: 0.34722222rem;
}
.content .content_top ul > li:nth-child(3) a {
  text-indent: 0.13888889rem;
}
.content_bottom {
  width: 100%;
}
.content_bottom .content_bottom_bottom {
  width: 100%;
  margin-top: 0.34722222rem;
}
.content_bottom .content_bottom_bottom .box {
  width: 100%;
  border: 0.01388889rem solid #e5e5e5;
  margin-bottom: 0.41666667rem;
}
.content_bottom .content_bottom_bottom .box .box_top {
  position: relative;
  margin: 0.27777778rem;
}
.content_bottom .content_bottom_bottom .box .box_top .l a {
  display: inline-block;
  width: 2.22222222rem;
  height: 2.22222222rem;
  background: url("../img/waimai/waimai01.png") no-repeat;
  background-size: 2.22222222rem 2.22222222rem;
}
.content_bottom .content_bottom_bottom > .box:nth-child(2) .box_top .l a {
  background: url("../img/waimai/waimai02.png") no-repeat;
  background-size: 2.22222222rem 2.22222222rem;
}
.content_bottom .content_bottom_bottom > .box:nth-child(3) .box_top .l a {
  background: url("../img/waimai/waimai03.png") no-repeat;
  background-size: 2.22222222rem 2.22222222rem;
}
.content_bottom .content_bottom_bottom .box .box_top .m {
  margin-left: 0.41666667rem;
  color: #000000;
  height: 2.22222222rem;
}
.content_bottom .content_bottom_bottom .box .box_top .m h1 {
  font-size: 0.41666667rem;
  margin-top: 0.20833333rem;
}
.content_bottom .content_bottom_bottom .box .box_top .m p {
  font-size: 0.34722222rem;
  margin: 0.27777778rem 0;
}
.content_bottom .content_bottom_bottom .box .box_top .m p:nth-child(3) {
  margin: 0;
}
.content_bottom .content_bottom_bottom .box .box_top .a {
  position: absolute;
  right: 0;
  top: 0.27777778rem;
  font-size: 0.27777778rem;
  color: #ffffff;
  padding: 0.06944444rem;
  background: #f2bd47;
}
.content_bottom .content_bottom_bottom .box .box_top .ding {
  right: 0.625rem;
  background: #017e5c;
}
.content_bottom .content_bottom_bottom .box .box_bottom {
  background: #eeeeee;
  color: #a6a6a6;
  font-size: 0.41666667rem;
  line-height: 1.05555556rem;
  padding-left: 0.27777778rem;
}
/* 尾部 */
/* 模态窗口 */
.mask {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  filter: alpha(opacity=50);
  position: fixed;
  top: 2.29166667rem;
  left: 0;
  z-index: 2;
}
.echo {
  width: 10rem;
  border-top: 1px solid #dedede;
  position: absolute;
  z-index: 2;
  background: #f9f9f9;
  left: 0;
  top: 0.90277778rem;
  padding: 0.27777778rem 0 0.34722222rem 0;
}
.echo ul {
  margin-bottom: 0.34722222rem;
}
.echo ul li {
  float: left;
  border: 0.01388889rem solid #dbdbdb;
  margin: 0 0.29166667rem 0.30555556rem 0;
}
.echo ul > li:nth-child(3) {
  margin-right: 0;
}
.echo ul > li:nth-child(6) {
  margin-right: 0;
}
.echo ul > li:nth-child(9) {
  margin-right: 0;
}
.echo ul > li:nth-child(12) {
  margin-right: 0;
}
.echo ul > li:nth-child(15) {
  margin-right: 0;
}
.echo ul li a {
  display: inline-block;
  font-size: 0.34722222rem;
  color: #a6a6a6;
  line-height: 1.25rem;
  width: 2.91666667rem;
  text-align: center;
  background: #ffffff;
}
.echo .title {
  border-top: 0.01388889rem solid #d9d8d8;
  border-bottom: 0.01388889rem solid #d9d8d8;
}
.echo .title p {
  font-size: 0.34722222rem;
  color: #a6a6a6;
  line-height: 1.06944444rem;
}
.echo .title > p:nth-child(1) {
  padding-left: 0.94444444rem;
  background: url("../img/waimai/dqcity.png") no-repeat left;
  background-size: 0.94444444rem 1.06944444rem;
}
.echo .title a {
  display: block;
  float: right;
  font-size: 0.34722222rem;
  color: #a6a6a6;
  line-height: 1.06944444rem;
  padding-right: 0.80555556rem;
  background: url("../img/waimai/qiehuan.png") no-repeat right;
  background-size: 0.80555556rem 1.06944444rem;
}
.echo,
.mask {
  display: none;
}
